
<% metaData=` <title>404: Are you lost? :(</title>
    <meta name="title" content="404: Are you lost? :(" />
    <meta name="description"
        content="404: Are you lost? :(" />
    `;
    %>
    
    <section class="w-full hero-section stars relative hero-section overflow-hidden h-[800px] flex flex-col justify-center items-center">
    <div class="twinkling"></div>
    <div class="gradient-overlay"></div>


        <div class="saturate-200 space-fog-1 absolute opacity-40 left-1/3 top-1/4 -rotate-12 transform -translate-x-1/2 -translate-y-1/2 z-20 w-[1000px] md:w-[2000px] h-auto aspect-square bg-center bg-no-repeat bg-contain no-drag"
            style="background-image: url('./img/bg-accent-1.webp');"></div>
        <div class="saturate-200 space-fog-2 absolute opacity-40 hidden md:block top-2/4 rotate-180 transform -translate-x-1/2 -translate-y-1/2 z-20 w-[1000] md:w-[2000px] h-auto aspect-square bg-center bg-no-repeat bg-contain no-drag"
            style="background-image: url('./img/bg-accent-1.webp');"></div>


    <div class="container px-8 mx-auto relative z-30">
        <div class="flex flex-col gap-8 lg:justify-center">

            <h1 class="md:text-9xl text-7xl font-bold tracking-tight text-white text-center">
                404
            </h1>

            <h2 class="md:text-6xl text-5xl font-bold tracking-tight text-white text-center">
                You might have lost your way.
            </h2>
            <p class="text-sm md:text-lg leading-6 text-gray-300 text-center">
                The page you're looking for doesn't exist anymore.
            </p>

            <div class="w-full flex justify-center gap-16">
                <div class="w-fit flex items-center justify-center flex-col gap-4">
                    <!-- <p class="text-sm md:text-lg leading-6 text-gray-300 text-center">
                        Sign up now:
                    </p> -->
                    <%- include ('./partials/social-connect.ejs') %>
                </div>

            </div>
        </div>

    </div>
    <div class="absolute bottom-0 left-1/2 -translate-x-1/2 -translate-y-1/2 container h-[1px] mx-auto bg-white/10 gradient-mask-x"></div>

</section>

<script src="./js/features.js"></script>
